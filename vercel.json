{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "back-end/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "back-end/news.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/index.html",
      "dest": "/public/index.html"
    },
    {
      "src": "/api/server",
      "dest": "/back-end/server.js"
    },
    {
      "src": "/api/news",
      "dest": "/back-end/news.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
